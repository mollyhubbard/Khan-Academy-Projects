CREATE TABLE fitness_pal (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT,
    workout_type TEXT,
    minutes INTEGER
    );
INSERT INTO fitness_pal (name, workout_type, minutes) VALUES ("Katrina", "running", 50);
